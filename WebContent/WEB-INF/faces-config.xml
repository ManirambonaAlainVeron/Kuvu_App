<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>mo</managed-bean-name>
  <managed-bean-class>kuvunja.Monnaie</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>ven</managed-bean-name>
  <managed-bean-class>kuvunja.Vente</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>index</managed-bean-name>
  <managed-bean-class>kuvunja.Index</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>echange</managed-bean-name>
  <managed-bean-class>kuvunja.Echange</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/Index.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>echange</from-outcome>
   <to-view-id>/Monnaie.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/*</from-view-id>
  <navigation-case>
   <from-outcome>new_monaie</from-outcome>
   <to-view-id>/Monnaie.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>new_vente</from-outcome>
   <to-view-id>/Vente.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>new_echange</from-outcome>
   <to-view-id>/Echange.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>new_index</from-outcome>
   <to-view-id>/Index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
